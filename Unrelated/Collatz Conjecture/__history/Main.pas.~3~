  TAutoSizeArray<T> = class (TArray<T>)
  protected
    function GetItem(AIndex: Integer): T; override;
    procedure SetItem(AIndex: Integer; AValue: T); override;


  end;

  TForm1 = class(TForm)
  private
    Numbers: TArray<Integer>;
  end;

var
  Form1: TForm1;

